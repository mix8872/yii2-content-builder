{
    "name": "mix8872/yii2-content-builder",
    "description": "The module is designed for simplified construction of site pages.",
	"version": "0.1.0",
    "type": "yii2-extension",
    "keywords": ["yii2","extension"],
    "license": "GPL-3.0+",
    "authors": [
        {
            "name": "mix8872",
            "email": "mix8872@gmail.com"
        },
		{
            "name": "infernotiv",
            "email": "shevkoplias.mk@yandex.ru"
        }
    ],
    "require": {
		"php": ">=7.0.0",
        "yiisoft/yii2": "~2.0.0",
        "ralouphie/getallheaders": "~2.0",
        "psr/http-message": "dev-master",
		"psr/log": "dev-master",
		"psr/container": "dev-master",
		"psr/cache": "dev-master",
        "guzzlehttp/psr7": "dev-master",
        "guzzlehttp/promises": "dev-master",
        "guzzlehttp/guzzle": "dev-master",
        "spatie/dropbox-api": "~1.6",
        "spatie/flysystem-dropbox": "~1.0",
        "microsoft/azure-storage": "dev-master",
		"league/flysystem": "~1.0",
        "league/flysystem-azure": "dev-master",
		"league/flysystem-aws-s3-v3": "~1.0",
        "league/flysystem-cached-adapter": "~1.0",
        "mtdowling/jmespath.php": "~2.4",
        "aws/aws-sdk-php": "~3.62",
        "monolog/monolog": "1.x-dev",
        "pimple/pimple": "dev-master",
        "symfony/polyfill-mbstring": "dev-master",
        "symfony/http-foundation": "2.7.x-dev",
        "symfony/event-dispatcher": "2.7.x-dev",
        "symfony/debug": "2.8.x-dev",
        "symfony/polyfill-ctype": "1.9.0",
        "symfony/http-kernel": "2.7.x-dev"
    },
    "autoload": {
        "psr-0": {
            "CKSource": "src/"
        },
        "psr-4": {
            "CKSource\\CKFinder\\Plugin\\": "plugins/"
        }
    }
}
