{
    "name": "mix8872/yii2-content-builder",
    "description": "The module is designed for simplified construction of site pages.",
	"version": "0.1.0",
    "type": "yii2-extension",
    "keywords": ["yii2","extension"],
    "license": "GPL-3.0+",
    "authors": [
        {
            "name": "mix8872",
            "email": "mix8872@gmail.com"
        },
		{
            "name": "infernotiv",
            "email": "shevkoplias.mk@yandex.ru"
        }
    ],
    "require": {
		"php": ">=7.0.0",
        "yiisoft/yii2": "~2.0.0",
        "ralouphie/getallheaders": "~2.0",
        "psr/http-message": "~1.0",
		"psr/log": "~1.0",
		"psr/container": "~1.0",
		"psr/cache": "~1.0",
        "guzzlehttp/psr7": "~1.4",
        "guzzlehttp/promises": "~1.4",
        "guzzlehttp/guzzle": "~6.3",
        "spatie/dropbox-api": "~1.6",
        "spatie/flysystem-dropbox": "~1.0",
        "microsoft/azure-storage": "~0.19",
		"league/flysystem": "~1.0",
        "league/flysystem-azure": "~1.0",
		"league/flysystem-aws-s3-v3": "~1.0",
        "league/flysystem-cached-adapter": "~1.0",
        "mtdowling/jmespath.php": "~2.4",
        "aws/aws-sdk-php": "~3.62",
        "monolog/monolog": "~1",
        "pimple/pimple": "~3.2",
        "symfony/polyfill-mbstring": "~1.9",
        "symfony/http-foundation": "~2.7",
        "symfony/event-dispatcher": "~4.1",
        "symfony/debug": "~2.8",
        "symfony/polyfill-ctype": "1.9.0",
        "symfony/http-kernel": "~2.7"
    },
    "autoload": {
        "psr-0": {
            "CKSource": "src/"
        },
        "psr-4": {
            "CKSource\\CKFinder\\Plugin\\": "plugins/"
